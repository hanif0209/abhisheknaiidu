  }

  _input(String e, int i, String val) {
    int foundKey = -1;
    for (var map in _values!) {
      if (map.containsKey('index')) {
        foundKey = i;
        break;
      }
    }
    if (-1 != foundKey) {
      _values!.removeWhere((map) {
        return map['index'] == foundKey;
      });
    }
    Map<String, dynamic> json = {'index': i, 'key': e, 'value': val};
    _values!.add(json);
    setState(() {
      print(_values);
    });
  }
}
