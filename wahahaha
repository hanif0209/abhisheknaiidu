part of 'pages.dart';

class InputKelola extends StatefulWidget {
  final String? stepke;
  final String? laststep;
  final String? typeInput;
  final List<String>? jenis;
  final String? title;
  const InputKelola(
      {Key? key,
      this.stepke,
      this.laststep,
      this.typeInput,
      this.jenis,
      this.title})
      : super(key: key);

  @override
  _InputKelolaState createState() => _InputKelolaState();
}

class _InputKelolaState extends State<InputKelola> {
  Map<String, bool> roda2 = {
    'Motor': false,
    'Sepeda': false,
    'Delman': false,
  };
  Map<String, bool> roda3 = {
    'Becak': false,
    'Bajaj': false,
  };
  Map<String, bool> roda4 = {
    'Mobil': false,
    'Bus': false,
    'Truk': false,
  };
  Map<String, bool> bangunan = {
    'Rumah': false,
    'Kos - kosan ': false,
    'Kontrakan': false,
    'Apartment': false,
    'Gudang': false,
  };
  Map<String, bool> paket = {
    'Rumah': false,
    'Kos - kosan ': false,
    'Kontrakan': false,
    'Apartment': false,
  };

  Map<String, bool> pickup = {
    'Motor': false,
    'Mobil': false,
    'Mobil Pick Up': false,
    'Truk': false,
  };
  List<String> dataKendaraan = [];
  List<String> dataBangunan = [];
  List<String> dataBarangItem = [];
  List<String> dataBarangPaket = [];
  List<String> dataKendaraanPickup = [];

  var titik, provinsi, kabupaten, kecamatan;
  var provinsicek, kabupatencek, kecamatancek;

  int id = 0;

  SubLayanan? layanan;
  TextEditingController provinsiController = TextEditingController();
  TextEditingController kabupatenController = TextEditingController();
  TextEditingController kecamatanController = TextEditingController();
  TextEditingController luas1Controller = TextEditingController();
  TextEditingController luas2Controller = TextEditingController();
  TextEditingController catatanKapasitasController = TextEditingController();
  TextEditingController barangItemController = TextEditingController();

  void _navigateLocation(BuildContext context) async {
    titik = await Get.to(() => LocationAlamat());
    if (titik != null) {
      setState(() {});
      print(titik); // call your own function here to refresh screen
    }
  }

  void _navigateProvinsi(BuildContext context) async {
    await context.read<PilihanalamatCubit>().showProvinsi();

    provinsi = await Get.to(() => PilihAlamatPage('Provinsi'));
    provinsicek = (provinsi == null) ? "Ex . Udin Petot" : provinsi['name'];
    if (provinsi != null) {
      setState(() {});
      print(provinsi); // call your own function here to refresh screen
    }
  }

  void _navigateKabupaten(BuildContext context) async {
    await context.read<PilihanalamatCubit>().showKota(provinsi['key']);

    kabupaten = await Get.to(() => PilihAlamatPage('Kabupaten'));
    kabupatencek = (kabupaten == null) ? "Ex . Udin Petot" : kabupaten['name'];

    if (kabupaten != null) {
      setState(() {});

      print(kabupaten); // call your own function here to refresh screen
    }
  }

  void _navigateKecamatan(BuildContext context) async {
    await context.read<PilihanalamatCubit>().showKecamatan(kabupaten['key']);

    kecamatan = await Get.to(() => PilihAlamatPage('Kecamatan'));
    kecamatancek = (kecamatan == null) ? "Ex . Udin Petot" : kecamatan['name'];
    if (kecamatan != null) {
      setState(() {});

      print(kecamatan); // call your own function here to refresh screen
    }
  }

  Map<String, dynamic>? _alamatUmum;
  List<Map<String, dynamic>>? _values;
  Map<String, dynamic>? _dataKendaraan;
  Map<String, dynamic>? _ketersediaanGudang;

  @override
  void initState() {
    super.initState();
    _values = [];
    print(myPickup);
  }

  List<Map>? myProducts;
  List<Map>? myPickup;

  @override
  Widget build(BuildContext context) {
    if (widget.stepke! == widget.laststep && widget.typeInput == 'Kendaraan') {
      myProducts = List.generate(widget.jenis!.length,
          (index) => {"id": index, "name": widget.jenis![index]}).toList();
    }
    if (widget.stepke! == widget.laststep &&
        widget.typeInput == 'Pickup' &&
        dataKendaraanPickup.length != 0) {
      myPickup = List.generate(dataKendaraanPickup.length,
              (index) => {"id": index, "name": dataKendaraanPickup[index]})
          .toList();
    }
    int index = 0;

    return SafeArea(
      child: Scaffold(
        bottomNavigationBar: Container(
          height: 11 * h,
          color: Colors.transparent,
          child: Center(
            child: Container(
              margin: EdgeInsets.fromLTRB(5 * w, 2 * h, 5 * w, 3.8 * h),
              height: 7.5 * h,
              width: double.infinity,
              child: ElevatedButton(
                  style: ElevatedButton.styleFrom(
                      elevation: 0, onPrimary: mainGreen2, primary: mainGreen2),
                  onPressed: () {
                    if (widget.typeInput == 'umum') {
                      if (titik == null) {
                        Get.snackbar('title', 'message');
                      } else if (provinsi == null) {
                        Get.snackbar('title', 'message');
                      } else if (kabupaten == null) {
                        Get.snackbar('title', 'message');
                      } else if (kecamatan == null) {
                        Get.snackbar('title', 'message');
                      } else {
                        _alamatUmum = {
                          'titik': titik['titik'],
                          'provinsi': provinsicek,
                          'kabupaten': kabupatencek,
                          'kecamatan': kecamatancek,
                          'alamat': titik['alamat']
                        };
                        Get.back(result: _alamatUmum);
                      }
                    } else if (widget.stepke! == widget.laststep &&
                        widget.typeInput == 'Kendaraan') {
                      _dataKendaraan = {
                        'luas_lahan': '' +
                            luas1Controller.text +
                            'x' +
                            luas2Controller.text,
                        'data': _values,
                        'catatan': catatanKapasitasController.text,
                      };
                      Get.back(result: _dataKendaraan);
                    } else if (widget.stepke! == '2' &&
                        widget.typeInput == 'Kendaraan') {
                      Get.back(result: dataKendaraan);
                    } else if (widget.typeInput == 'Bangunan' &&
                        widget.stepke! == widget.laststep) {
                      Get.back(result: dataBangunan);
                    } else if (widget.typeInput == 'BarangItem') {
                      Get.back(result: dataBarangItem);
                    } else if (widget.typeInput == 'KetersediaanGudang') {
                      _ketersediaanGudang = {
                        'luas_lahan': '' +
                            luas1Controller.text +
                            'x' +
                            luas2Controller.text,
                        'catatan': catatanKapasitasController.text
                      };
                      Get.back(result: _ketersediaanGudang);
                    } else if (widget.typeInput == 'BarangPaket') {
                      Get.back(result: dataBarangPaket);
                    } else if (widget.typeInput == 'Pickup') {
                      Get.back(result: _values);
                    }
                  },
                  child: Row(
                    mainAxisAlignment: MainAxisAlignment.center,
                    children: [
                      Text(
                        (widget.stepke != '1') ? "Lanjutkan" : 'Next',
                        style: blackFontStyle1.copyWith(
                            fontSize: 2.6 * sp,
                            color: Colors.white,
                            fontWeight: FontWeight.w600),
                      ),
                      SizedBox(
                        width: 1.2 * h,
                      ),
                      (widget.stepke != '1')
                          ? SizedBox()
                          : Icon(Icons.arrow_forward_ios_outlined)
                    ],
                  )),
            ),
          ),
        ),
        body: CustomScrollView(
          slivers: [
            SliverAppBar(
              elevation: 0,
              backgroundColor: Colors.white,
              pinned: true,
              automaticallyImplyLeading: false,
              flexibleSpace: Container(
                margin: EdgeInsets.all(2.2 * h),
                child: Row(
                  mainAxisAlignment: MainAxisAlignment.spaceBetween,
                  children: [
                    Row(
                      children: [
                        Icon(
                          Icons.arrow_back_rounded,
                          color: mainGreen2,
                        ),
                        SizedBox(width: 4 * w),
                        Text(
                          widget.title ?? "title",
                          style: blackFontStyle1.copyWith(fontSize: 2 * sp),
                        )
                      ],
                    ),
                    Text(
                      "Langkah ${widget.stepke} dari ${widget.laststep}",
                      style: blackFontStyle1.copyWith(
                          fontSize: 1.4 * sp, color: greycolor2),
                    )
                  ],
                ),
              ),
            ),
            // Input Alamat Umum
            (widget.typeInput == 'umum')
